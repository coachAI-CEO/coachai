generator client { provider = "prisma-client-js" }
datasource db { provider = "postgresql" url = env("DATABASE_URL") }

model Club {
  id        String   @id @default(cuid())
  name      String
  createdAt DateTime @default(now())
  teams     Team[]
}

model Team {
  id        String   @id @default(cuid())
  clubId    String
  name      String
  ageBand   String
  createdAt DateTime @default(now())
  club      Club     @relation(fields: [clubId], references: [id])
}
